@model IEnumerable<ToDoApp.Models.ToDo>

@{
    ViewData["Title"] = "Index";
}

<h2>Status</h2>

<p>
    <a asp-action="Create">Create New Status</a>
</p>

<p>
    Current Statuses:
</p>

<ul>
    @foreach (var item in Model)
    {
        <li>
            @Html.DisplayFor(modelItem => item.Status)
        </li>
    }
</ul>



